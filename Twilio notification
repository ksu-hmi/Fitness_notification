from twilio.rest import Client 
 
account_sid = 'ACd5808dd8d2927ac7e47284fe76d1449b' 
auth_token = '[AuthToken]' 
client = Client(account_sid, auth_token) 
 
message = client.messages.create(  
                              messaging_service_sid='MG3ed9c41e5579a580ad47221c103d6f93', 
                              body='Your class is today',      
                              to='+12294490242' 
                          ) 
 
print(message.sid)
